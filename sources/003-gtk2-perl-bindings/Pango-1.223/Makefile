# --- MakeMaker clean section:

# Delete temporary files but do not touch installed files. We don't delete
# the Makefile here so a later make realclean still has a makefile to use.

clean :: clean_subdirs
	- $(RM_F) \
	  core.[0-9] $(BASEEXT).bso \
	  pm_to_blib.ts xs/PangoGravity.c \
	  core.[0-9][0-9][0-9][0-9] MYMETA.yml \
	  xs/PangoTabs.c perl$(EXE_EXT) \
	  tmon.out *$(OBJ_EXT) \
	  pm_to_blib blibdirs.ts \
	  xs/PangoCairo.c *perl.core \
	  core.*perl.*.? $(BASEEXT).def \
	  core.[0-9][0-9][0-9] mon.out \
	  lib$(BASEEXT).def perl.exe \
	  perlmain.c xs/PangoContext.c \
	  so_locations $(BASEEXT).exp \
	  xs/PangoFontMap.c xs/PangoAttributes.c \
	  *$(LIB_EXT) core \
	  xs/PangoFontset.c core.[0-9][0-9] \
	  $(INST_ARCHAUTODIR)\extralibs.ld MYMETA.json \
	  $(BASEEXT).x $(BOOTSTRAP) \
	  $(INST_ARCHAUTODIR)\extralibs.all xs/PangoTypes.c \
	  xs/Pango.c core.[0-9][0-9][0-9][0-9][0-9] \
	  xs/PangoMatrix.c xs/PangoFont.c \
	  $(MAKE_APERL_FILE) perl \
	  xs/PangoLayout.c xs/PangoRenderer.c \
	  xs/PangoScript.c 
	- $(RM_RF) \
	  xs/PangoCairo$(OBJ_EXT) xs/PangoTypes$(OBJ_EXT) \
	  xs/PangoRenderer$(OBJ_EXT) xs/PangoContext$(OBJ_EXT) \
	  xs/PangoFontset$(OBJ_EXT) xs/PangoScript$(OBJ_EXT) \
	  dll.exp xs/PangoAttributes$(OBJ_EXT) \
	  xs/PangoTabs$(OBJ_EXT) xs/PangoLayout$(OBJ_EXT) \
	  xs/PangoFontMap$(OBJ_EXT) dll.base \
	  xs/Pango$(OBJ_EXT) xs/PangoFont$(OBJ_EXT) \
	  xs/PangoMatrix$(OBJ_EXT) blib \
	  xs/PangoGravity$(OBJ_EXT) 
	- $(MV) $(FIRST_MAKEFILE) $(MAKEFILE_OLD) $(DEV_NULL)


# --- MakeMaker realclean_subdirs section:
realclean_subdirs :
	$(NOECHO) $(NOOP)


# --- MakeMaker realclean section:
# Delete temporary files (via clean) and also delete dist files
realclean purge ::  clean realclean_subdirs
	- $(RM_F) \
	  $(OBJECT) $(MAKEFILE_OLD) \
	  $(FIRST_MAKEFILE) 
	- $(RM_RF) \
	  $(DISTVNAME) 


